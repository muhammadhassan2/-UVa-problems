#include <iostream>
using namespace std;
int main()
{
int n;
for(int a=6;a<=200;a++){
for(int i=2;i<=200;i++)
{
    for(int j=i;j<=200;j++)
    {
        for(int k=j;k<=200;k++)
        {
            if(i*i*i+j*j*j+k*k*k==a*a*a)
            {
                cout<<"Cube = "<<a<<", "<<"Triple = ("<<i<<","<<j<<","<<k<<")"<<endl;
            }
        }
    }
}
}
    return 0;

}
